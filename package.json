{
  "name": "login",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "yarn build && node build/index.js",
    "start:dev": "nodemon -L -e ts --exec \"yarn start\"",
    "start:db": "docker-compose up db",
    "build": "rimraf ./build && tsc && ( sequelize-cli init || echo 'Skipping sequelize-cli init' ) && sequelize-cli db:create && sequelize-cli db:migrate --table users",
    "dc:up": "docker-compose up",
    "dc:down": "docker-compose down",
    "dc:build": "docker-compose build"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.13",
    "@types/pg": "^8.6.6",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "nodemon": "^2.0.22",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "rimraf": "^5.0.0",
    "sequelize": "^6.31.0",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "sequelize-cli": "^6.6.0"
  },
  "engines": {
    "node": "16.20.0"
  }
}
